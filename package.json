    "@jl-org/vite-auto-route": "^2.0.1",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.20",
    "jsdom": "^24.1.3",
